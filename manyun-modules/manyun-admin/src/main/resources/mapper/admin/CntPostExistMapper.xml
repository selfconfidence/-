<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.manyun.admin.mapper.CntPostExistMapper">

    <resultMap type="CntPostExist" id="CntPostExistResult">
        <result property="id"    column="id"    />
        <result property="collectionId"    column="collection_id"    />
        <result property="configId"    column="config_id"    />
        <result property="buiName"    column="bui_name"    />
        <result property="createdBy"    column="created_by"    />
        <result property="createdTime"    column="created_time"    />
        <result property="updatedBy"    column="updated_by"    />
        <result property="updatedTime"    column="updated_time"    />
    </resultMap>

    <sql id="selectCntPostExistVo">
        select id, collection_id, config_id, bui_name, created_by, created_time, updated_by, updated_time from cnt_post_exist
    </sql>

    <select id="selectCntPostExistList" parameterType="CntPostExist" resultMap="CntPostExistResult">
        <include refid="selectCntPostExistVo"/>
        <where>
            <if test="collectionId != null  and collectionId != ''"> and collection_id = #{collectionId}</if>
            <if test="configId != null  and configId != ''"> and config_id = #{configId}</if>
            <if test="buiName != null  and buiName != ''"> and bui_name like concat('%', #{buiName}, '%')</if>
            <if test="createdBy != null  and createdBy != ''"> and created_by = #{createdBy}</if>
            <if test="createdTime != null "> and created_time = #{createdTime}</if>
            <if test="updatedBy != null  and updatedBy != ''"> and updated_by = #{updatedBy}</if>
            <if test="updatedTime != null "> and updated_time = #{updatedTime}</if>
        </where>
        order by created_time desc
    </select>

</mapper>
