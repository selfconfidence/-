<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.manyun.admin.mapper.CntUserCollectionMapper">
    
    <resultMap type="CntUserCollection" id="CntUserCollectionResult">
        <result property="id"    column="id"    />
        <result property="userId"    column="user_id"    />
        <result property="collectionId"    column="collection_id"    />
        <result property="collectionName"    column="collection_name"    />
        <result property="collectionNumber"    column="collection_number"    />
        <result property="collectionHash"    column="collection_hash"    />
        <result property="linkAddr"    column="link_addr"    />
        <result property="isLink"    column="is_link"    />
        <result property="sourceInfo"    column="source_info"    />
        <result property="isExist"    column="is_exist"    />
        <result property="realCompany"    column="real_company"    />
        <result property="createdBy"    column="created_by"    />
        <result property="createdTime"    column="created_time"    />
        <result property="updatedBy"    column="updated_by"    />
        <result property="updatedTime"    column="updated_time"    />
    </resultMap>

    <sql id="selectCntUserCollectionVo">
        select id, user_id, collection_id, collection_name, collection_number, collection_hash, link_addr, is_link, source_info, is_exist, real_company, created_by, created_time, updated_by, updated_time from cnt_user_collection
    </sql>

    <select id="selectCntUserCollectionList" parameterType="CntUserCollection" resultMap="CntUserCollectionResult">
        <include refid="selectCntUserCollectionVo"/>
        <where>  
            <if test="userId != null  and userId != ''"> and user_id = #{userId}</if>
            <if test="collectionId != null  and collectionId != ''"> and collection_id = #{collectionId}</if>
            <if test="collectionName != null  and collectionName != ''"> and collection_name like concat('%', #{collectionName}, '%')</if>
            <if test="collectionNumber != null  and collectionNumber != ''"> and collection_number = #{collectionNumber}</if>
            <if test="collectionHash != null  and collectionHash != ''"> and collection_hash = #{collectionHash}</if>
            <if test="linkAddr != null  and linkAddr != ''"> and link_addr = #{linkAddr}</if>
            <if test="isLink != null "> and is_link = #{isLink}</if>
            <if test="sourceInfo != null  and sourceInfo != ''"> and source_info = #{sourceInfo}</if>
            <if test="isExist != null "> and is_exist = #{isExist}</if>
            <if test="realCompany != null  and realCompany != ''"> and real_company = #{realCompany}</if>
            <if test="createdBy != null  and createdBy != ''"> and created_by = #{createdBy}</if>
            <if test="createdTime != null "> and created_time = #{createdTime}</if>
            <if test="updatedBy != null  and updatedBy != ''"> and updated_by = #{updatedBy}</if>
            <if test="updatedTime != null "> and updated_time = #{updatedTime}</if>
        </where>
    </select>
    
    <select id="selectCntUserCollectionById" parameterType="String" resultMap="CntUserCollectionResult">
        <include refid="selectCntUserCollectionVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertCntUserCollection" parameterType="CntUserCollection">
        insert into cnt_user_collection
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="userId != null">user_id,</if>
            <if test="collectionId != null">collection_id,</if>
            <if test="collectionName != null">collection_name,</if>
            <if test="collectionNumber != null">collection_number,</if>
            <if test="collectionHash != null">collection_hash,</if>
            <if test="linkAddr != null">link_addr,</if>
            <if test="isLink != null">is_link,</if>
            <if test="sourceInfo != null">source_info,</if>
            <if test="isExist != null">is_exist,</if>
            <if test="realCompany != null">real_company,</if>
            <if test="createdBy != null">created_by,</if>
            <if test="createdTime != null">created_time,</if>
            <if test="updatedBy != null">updated_by,</if>
            <if test="updatedTime != null">updated_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="userId != null">#{userId},</if>
            <if test="collectionId != null">#{collectionId},</if>
            <if test="collectionName != null">#{collectionName},</if>
            <if test="collectionNumber != null">#{collectionNumber},</if>
            <if test="collectionHash != null">#{collectionHash},</if>
            <if test="linkAddr != null">#{linkAddr},</if>
            <if test="isLink != null">#{isLink},</if>
            <if test="sourceInfo != null">#{sourceInfo},</if>
            <if test="isExist != null">#{isExist},</if>
            <if test="realCompany != null">#{realCompany},</if>
            <if test="createdBy != null">#{createdBy},</if>
            <if test="createdTime != null">#{createdTime},</if>
            <if test="updatedBy != null">#{updatedBy},</if>
            <if test="updatedTime != null">#{updatedTime},</if>
         </trim>
    </insert>

    <update id="updateCntUserCollection" parameterType="CntUserCollection">
        update cnt_user_collection
        <trim prefix="SET" suffixOverrides=",">
            <if test="userId != null">user_id = #{userId},</if>
            <if test="collectionId != null">collection_id = #{collectionId},</if>
            <if test="collectionName != null">collection_name = #{collectionName},</if>
            <if test="collectionNumber != null">collection_number = #{collectionNumber},</if>
            <if test="collectionHash != null">collection_hash = #{collectionHash},</if>
            <if test="linkAddr != null">link_addr = #{linkAddr},</if>
            <if test="isLink != null">is_link = #{isLink},</if>
            <if test="sourceInfo != null">source_info = #{sourceInfo},</if>
            <if test="isExist != null">is_exist = #{isExist},</if>
            <if test="realCompany != null">real_company = #{realCompany},</if>
            <if test="createdBy != null">created_by = #{createdBy},</if>
            <if test="createdTime != null">created_time = #{createdTime},</if>
            <if test="updatedBy != null">updated_by = #{updatedBy},</if>
            <if test="updatedTime != null">updated_time = #{updatedTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteCntUserCollectionById" parameterType="String">
        delete from cnt_user_collection where id = #{id}
    </delete>

    <delete id="deleteCntUserCollectionByIds" parameterType="String">
        delete from cnt_user_collection where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>