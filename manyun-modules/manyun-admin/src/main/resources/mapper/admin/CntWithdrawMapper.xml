<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.manyun.admin.mapper.CntWithdrawMapper">
    
    <resultMap type="CntWithdraw" id="CntWithdrawResult">
        <result property="id"    column="id"    />
        <result property="state"    column="state"    />
        <result property="servicefee"    column="servicefee"    />
        <result property="minmoney"    column="minmoney"    />
        <result property="monthlimit"    column="monthlimit"    />
        <result property="rule"    column="rule"    />
        <result property="createdBy"    column="created_by"    />
        <result property="createdTime"    column="created_time"    />
        <result property="updatedBy"    column="updated_by"    />
        <result property="updatedTime"    column="updated_time"    />
    </resultMap>

    <sql id="selectCntWithdrawVo">
        select id, state, servicefee, minmoney, monthlimit, rule, created_by, created_time, updated_by, updated_time from cnt_withdraw
    </sql>

    <select id="selectCntWithdrawList" parameterType="CntWithdraw" resultMap="CntWithdrawResult">
        <include refid="selectCntWithdrawVo"/>
        <where>  
            <if test="state != null  and state != ''"> and state = #{state}</if>
            <if test="servicefee != null "> and servicefee = #{servicefee}</if>
            <if test="minmoney != null "> and minmoney = #{minmoney}</if>
            <if test="monthlimit != null "> and monthlimit = #{monthlimit}</if>
            <if test="rule != null  and rule != ''"> and rule = #{rule}</if>
            <if test="createdBy != null  and createdBy != ''"> and created_by = #{createdBy}</if>
            <if test="createdTime != null "> and created_time = #{createdTime}</if>
            <if test="updatedBy != null  and updatedBy != ''"> and updated_by = #{updatedBy}</if>
            <if test="updatedTime != null "> and updated_time = #{updatedTime}</if>
        </where>
    </select>
    
    <select id="selectCntWithdrawById" parameterType="String" resultMap="CntWithdrawResult">
        <include refid="selectCntWithdrawVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertCntWithdraw" parameterType="CntWithdraw">
        insert into cnt_withdraw
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="state != null">state,</if>
            <if test="servicefee != null">servicefee,</if>
            <if test="minmoney != null">minmoney,</if>
            <if test="monthlimit != null">monthlimit,</if>
            <if test="rule != null">rule,</if>
            <if test="createdBy != null">created_by,</if>
            <if test="createdTime != null">created_time,</if>
            <if test="updatedBy != null">updated_by,</if>
            <if test="updatedTime != null">updated_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="state != null">#{state},</if>
            <if test="servicefee != null">#{servicefee},</if>
            <if test="minmoney != null">#{minmoney},</if>
            <if test="monthlimit != null">#{monthlimit},</if>
            <if test="rule != null">#{rule},</if>
            <if test="createdBy != null">#{createdBy},</if>
            <if test="createdTime != null">#{createdTime},</if>
            <if test="updatedBy != null">#{updatedBy},</if>
            <if test="updatedTime != null">#{updatedTime},</if>
         </trim>
    </insert>

    <update id="updateCntWithdraw" parameterType="CntWithdraw">
        update cnt_withdraw
        <trim prefix="SET" suffixOverrides=",">
            <if test="state != null">state = #{state},</if>
            <if test="servicefee != null">servicefee = #{servicefee},</if>
            <if test="minmoney != null">minmoney = #{minmoney},</if>
            <if test="monthlimit != null">monthlimit = #{monthlimit},</if>
            <if test="rule != null">rule = #{rule},</if>
            <if test="createdBy != null">created_by = #{createdBy},</if>
            <if test="createdTime != null">created_time = #{createdTime},</if>
            <if test="updatedBy != null">updated_by = #{updatedBy},</if>
            <if test="updatedTime != null">updated_time = #{updatedTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteCntWithdrawById" parameterType="String">
        delete from cnt_withdraw where id = #{id}
    </delete>

    <delete id="deleteCntWithdrawByIds" parameterType="String">
        delete from cnt_withdraw where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>